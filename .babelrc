{
  "presets": [
    [
      "@babel/env",
      {
        "corejs": 3,
        "useBuiltIns": "usage",
        "debug": true,
        "modules": false
      }
    ],
    [
      "@babel/preset-react",
      {
        "runtime": "automatic",
        "throwIfNamespace": false
      }
    ]
  ],
  "plugins": ["@babel/proposal-class-properties"]
}
