{
  "name": "token-table-viewer",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@chakra-ui/react": "^2.8.2",
    "@emotion/react": "^11.11.3",
    "@emotion/styled": "^11.11.0",
    "framer-motion": "^10.18.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "scripts": {
    "start": "webpack --mode=development --watch",
    "build": "webpack --mode=production",
    "lint:check": "npx eslint src/",
    "lint:fix": "npx eslint --fix src/",
    "lint:css": "stylelint src",
    "lint:css:fix": "npm run lint:css -- --fix",
    "prepare": "husky install"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "**/*.{css,scss}": "yarn run stylelint --fix",
    "**/*.{js,jsx,ts,tsx}": "yarn run prettier --write"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "parser": "@typescript-eslint/parser",
    "overrides": [
      {
        "files": [
          "**/*.stories.*"
        ],
        "rules": {
          "import/no-anonymous-default-export": "off"
        }
      }
    ]
  },
  "parser": "@typescript-eslint/parser",
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@figma/plugin-typings": "^1.84.0",
    "@figma/widget-typings": "^1.9.1",
    "@types/node": "^16.18.72",
    "@types/react": "^18.2.48",
    "@types/react-dom": "^18.2.18",
    "@types/sass": "^1.45.0",
    "@types/sass-loader": "^8.0.8",
    "babel-eslint": "^10.1.0",
    "css-loader": "^6.9.1",
    "eslint": "^8.56.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^17.1.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-config-react-app": "^7.0.1",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-import-resolver-babel-module": "^5.3.2",
    "eslint-plugin-eslint-plugin": "^5.2.1",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-no-loops": "^0.3.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-sonarjs": "^0.23.0",
    "eslint-plugin-storybook": "^0.6.15",
    "husky": "^8.0.0",
    "lint-staged": "^15.2.0",
    "postcss-loader": "^8.0.0",
    "sass": "^1.66.1",
    "sass-loader": "^13.3.2",
    "style-loader": "^3.3.4",
    "stylelint": "^15.10.3",
    "stylelint-config-clean-order": "^1.0.0",
    "stylelint-config-prettier": "^8.0.2",
    "stylelint-config-rational-order": "^0.0.4",
    "stylelint-config-standard": "^22.0.0",
    "stylelint-config-standard-scss": "^5.0.0",
    "stylelint-order": "^4.1.0",
    "stylelint-prettier": "^1.2.0",
    "stylelint-scss": "^3.20.1",
    "tsconfig-paths-webpack-plugin": "^4.1.0",
    "typescript": "^4.9.5",
    "web-vitals": "^3.5.1",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4"
  }
}
